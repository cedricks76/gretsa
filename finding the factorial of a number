def factorial(n):
    if n == 0 or n == 1:
        return 1
    else:
        result = 1
        for i in range(1, n + 1):
            result *= i
        return result

if __name__ == "__main__":
    num = 5  # You can change this number to calculate the factorial of a different value
    print(f"The factorial of {num} is {factorial(num)}")
